<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>LogException</Title>
      <Author>LonelyExplorer</Author>
      <Description>日志输出: Exception</Description>
      <Shortcut>logex</Shortcut>
    </Header>
    <Snippet>
      <Code Language="CSharp">
        <![CDATA[LE_Log.Log.Exception($exception$,"$tag$","$info$");]]>
      </Code>
      <Declarations>
        <Literal>
          <ID>exception</ID>
          <ToolTip>异常</ToolTip>
          <Default>new Exception()</Default>
        </Literal>
        <Literal>
          <ID>tag</ID>
          <ToolTip>信息类型</ToolTip>
          <Default>debug</Default>
        </Literal>
        <Literal>
          <ID>info</ID>
          <ToolTip>信息内容</ToolTip>
          <Default>data</Default>
        </Literal>
      </Declarations>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>

